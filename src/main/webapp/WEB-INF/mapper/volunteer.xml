<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="memVol">

	<!-- 회원 자원봉사 리스트 가져오기 -->
	<select id="selectVolunTeer" parameterType="VolunteerVO" resultType="VolunteerVO">
		SELECT title, mnum, date 
		FROM volunteer
		WHERE mnum = #{mnum}
	</select>
	
</mapper>