<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="bbs">
	<!-- 각 게시판의 셀렉문을 구해서 합치기.. -->
	<select id="selectMoney" parameterType="DonaVO"
		resultType="DonaVO">
		SELECT sum(donation)
		FROM donation
		WHERE (select date(now())
		)
	</select>
</mapper>